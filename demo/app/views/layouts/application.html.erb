<!DOCTYPE html>
<html>
<head>
  <title>Demo</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="m-site-banner">
    <%= image_tag('data:image/jpeg;base64,', class: 'm-site-banner-logo') %>
    <h1 class="m-site-banner-title">
      <%= @page_title || 'Pug store direct' %>
    </h1>
  </div>
  <div class="m-main">
    <aside class="m-main-nav">
      <nav>
        <ul class="m-site-navigation">
          <li><a href="#">Home</a></li>
          <li><a href="#">One</a></li>
          <li><a href="#">Two</a></li>
          <li><a href="#">Three</a></li>
        </ul>
      </nav>
    </aside>
    <section class="m-main-content">
      <%= yield %>
    </section>
  </div>
</body>
</html>
